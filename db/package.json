{
  "name": "shukketsu-bugyo-db",
  "version": "1.0.0",
  "scripts": {
    "migrate": "sequelize-cli db:migrate --env development",
    "migrate:docker": "sequelize-cli db:migrate --env docker",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "create-migration": "sequelize-cli migration:generate --name",
    "create-model": "sequelize-cli model:generate --name",
    "migrate:full": "node migrate-with-docker.js"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "mysql2": "^3.15.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  }
}
